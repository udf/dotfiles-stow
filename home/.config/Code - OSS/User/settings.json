{
    "window.zoomLevel": -1,
    "editor.fontFamily": "'Hack Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.renderWhitespace": "all",
    "vim.handleKeys": {
        "<C-f>": false,
        "<C-d>": false
    },
    "explorer.confirmDelete": false,
    "vim.useSystemClipboard": true,
    "cSpell.userWords": [
        "HMAC",
        "botapi",
        "mt",
        "proto",
        "struct"
    ],
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": false,
    "vscode_custom_css.imports": [
        "file:///home/sam/.config/Code - OSS/vscode.css",
        "file:///home/mhoosen/.config/Code - OSS/vscode.css"
    ],
    "workbench.iconTheme": "material-icon-theme",
    "vscode_custom_css.policy": true,
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "editor.formatOnSave": false,
    "C_Cpp.clang_format_style": "{BasedOnStyle: LLVM, IndentWidth: 4, SpaceAfterTemplateKeyword: false, AlwaysBreakTemplateDeclarations: true, BinPackArguments: false, BinPackParameters: false, AllowAllParametersOfDeclarationOnNextLine: false, AllowShortFunctionsOnASingleLine: false, BreakBeforeBinaryOperators: \"NonAssignment\", AlignAfterOpenBracket: \"AlwaysBreak\"}",
    "workbench.editor.showTabs": true,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.rulers": [
        80,
        100
    ],
    "C_Cpp.updateChannel": "Insiders",
    "editor.renderControlCharacters": true,
    "C_Cpp.default.includePath": [
        "/usr/include/opencv4"
    ],
    "[c]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    // "update.mode": "none",
    // "python.autoUpdateLanguageServer": false,
    // "extensions.autoUpdate": false,
    // "extensions.autoCheckUpdates": false,
    "workbench.colorTheme": "Arc Dark Theme",
    "java.configuration.checkProjectSettingsExclusions": false,
    "diffEditor.maxComputationTime": 0,
    "vscode-neovim.neovimPath": "/usr/bin/nvim",
    "vscode-neovim.neovimInitPath": "~/.config/nvim/vscode.vim",
    "vscode-neovim.useCtrlKeysForInsertMode": false
}
