{
    "editor.fontFamily": "'Hack Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.renderWhitespace": "all",
    "explorer.confirmDelete": false,
    "cSpell.userWords": [
        "asyncio",
        "botapi",
        "centered",
        "centering",
        "craftable",
        "deepcopy",
        "desync",
        "Dialog",
        "elem",
        "flib",
        "fluidbox",
        "fluidboxes",
        "HMAC",
        "mt",
        "mult",
        "Playerctl",
        "proto",
        "redux",
        "romaji",
        "struct",
        "upvote",
        "vals"
    ],
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "vscode_custom_css.imports": [
        "file:///home/sam/.config/VScodium/vscode.css",
        "file:///home/mhoosen/.config/VScodium/vscode.css"
    ],
    "workbench.iconTheme": "material-icon-theme",
    "vscode_custom_css.policy": true,
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "editor.formatOnSave": false,
    "C_Cpp.clang_format_style": "{BasedOnStyle: LLVM, IndentWidth: 4, SpaceAfterTemplateKeyword: false, AlwaysBreakTemplateDeclarations: true, BinPackArguments: false, BinPackParameters: false, AllowAllParametersOfDeclarationOnNextLine: false, AllowShortFunctionsOnASingleLine: false, BreakBeforeBinaryOperators: \"NonAssignment\", AlignAfterOpenBracket: \"AlwaysBreak\"}",
    "workbench.editor.showTabs": "multiple",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.rulers": [
        80,
        100
    ],
    "C_Cpp.updateChannel": "Insiders",
    "editor.renderControlCharacters": true,
    "C_Cpp.default.includePath": [
        "/usr/include/opencv4"
    ],
    "[c]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    // "update.mode": "none",
    // "python.autoUpdateLanguageServer": false,
    // "extensions.autoUpdate": false,
    // "extensions.autoCheckUpdates": false,
    // "python.languageServer": "Pylance",
    "workbench.colorTheme": "Arc Dark Theme",
    "java.configuration.checkProjectSettingsExclusions": false,
    "diffEditor.maxComputationTime": 0,
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.linux": "~/.config/nvim/vscode.vim",
    "vscode-neovim.useCtrlKeysForInsertMode": false,
    "workbench.colorCustomizations": {
        "[Arc Dark Theme]": {
            "editorIndentGuide.activeBackground": "#666",
            "editor.lineHighlightBorder": "#232933",
            "editor.lineHighlightBackground": "#1f242d",
            "editor.findMatchHighlightBackground": "#70f8ff2a",
            "editorBracketMatch.background": "#ffffff40"
        }
    },
    "java.semanticHighlighting.enabled": true,
    "editor.tabSize": 2,
    "xml.java.home": "/usr/lib/jvm/default-runtime/",
    "[csharp]": {
        "editor.defaultFormatter": "Leopotam.csharpfixformat"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "python.showStartPage": false,
    "explorer.confirmDragAndDrop": false,
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-11",
            "path": "/usr/lib/jvm/java-11-openjdk/",
            "default": true
        }
    ],
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "http.proxySupport": "off",
    "terminal.integrated.inheritEnv": false,
    "[python]": {
        "gitlens.codeLens.symbolScopes": [
            "!Module"
        ],
        "editor.wordBasedSuggestions": "off"
    },
    "redhat.telemetry.enabled": false,
    "autoDocstring.docstringFormat": "sphinx",
    "editor.largeFileOptimizations": false,
    "nix.enableLanguageServer": true,
    "nix.serverSettings": {
        "nil": {
            "formatting": { "command": ["nixpkgs-fmt"] }
        }
    },
    "dance.modes": {
        "normal": {
            "cursorStyle": "block",
            "selectionBehavior": "character",
        },
    },
    "cSpell.language": "en-GB",
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "security.workspace.trust.enabled": false,
    "workbench.editor.navigationScope": "editorGroup",
    "workbench.editorAssociations": {
        "*.bin": "hexEditor.hexedit"
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "isort.args": [
        "--profile",
        "black",
        "--indent",
        "2",
        "--lai",
        "2",
        "--multi-line",
        "VERTICAL_GRID_GROUPED"
    ],
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "editor.inlineSuggest.enabled": false,
    "cSpell.enabled": true,
    "cSpell.enableFiletypes": [
        "lua"
    ],
    "window.zoomLevel": -1,
    "editor.stickyScroll.enabled": false,
}